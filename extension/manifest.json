{
  "manifest_version": 3,
  "name": "ToggleCtrlW - Toggle Switch for Ctrl + W",
  "short_name": "ToggleCtrlW",
  "description": "Toggle Switch for Ctrl + W.",
  "version": "0.1.0",
  "homepage_url": "https://github.com/kumarstack55/chrome-extension-toggle-ctrl-w",
  "permissions": ["storage", "activeTab"],
  "commands": {
    "command-ctrl-w": { "description": "Ctrl + W" }
  },
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "icons": {
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}
